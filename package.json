{
  "name": "corkei-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:hmr": "VITE_HMR=true vite",
    "build": "tsc -b && vite build",
    "build:debug": "tsc -b && vite build --debug",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -o github -m \"$(date +%Y-%m-%d_%H:%M:%S): Publish $(git rev-parse --short HEAD): $(git show --format='%s' -s)\""
  },
  "dependencies": {
    "d3": "^7.9.0",
    "jjsontree.js": "^4.7.1",
    "solid-js": "^1.9.9",
    "solid-motion": "^1.0.0-alpha.8",
    "solid-motionone": "^1.0.4"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/glob": "^8.1.0",
    "@types/node": "^25.0.6",
    "gh-pages": "^6.3.0",
    "glob": "^13.0.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vite-plugin-solid": "^2.11.8"
  }
}